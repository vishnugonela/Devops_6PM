pipeline{
        agent any
        stages{
                stage('stage1'){
                        steps{
                         sh 'echo "Build Successful from GITHUB" '
                        }
                }
                stage('stage2'){
                        steps{
                         sh '''
                              for i in {1..10}
                              do
                                 echo "Build running on `hostname`" 
                              done
                            '''
                        }
                }
}
}
