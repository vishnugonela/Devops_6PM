pipeline {
  agent any
  stages {
    stage('System Name') {
      steps {
        sh 'Name: echo -e "Hostname:\t\t\t"`hostname`'

      }
    }
    stage('System Memory') {
      steps {
        sh 'Memory: echo -e "Memory:\t\t"`free | awk '/Mem/{printf("%.2f"), $2/1024/1024}'`'
      }
    }
  }
}
